{
    "$schema": "http://json-schema.org/schema",
    "id": "SchematicsElectronBuild",
    "title": "Angular-Electron Build Options Schema",
    "type": "object",
    "properties": {
      "relatedApp": {
        "type": "string",
        "description": "Related application to build."
      },
      "webpackConfig": {
        "type": "string",
        "description": "The path to the Webpack configuration file."
      }
    },
    "required": [
      "relatedApp",
      "webpackConfig"
    ]
  }