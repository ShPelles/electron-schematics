{
  "$schema": "http://json-schema.org/schema",
  "id": "SchematicsElectron",
  "title": "Electron Options Schema",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the library.",
      "type": "string",
      "default": "electron"
    },
    "relatedAppName": {
      "description": "Name of related app.",
      "type": "string"
    },
    "skipInstall": {
      "description": "Skip installing dependency packages.",
      "type": "boolean",
      "default": false
    }
  },
  "required": []
}